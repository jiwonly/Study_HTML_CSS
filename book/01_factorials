  <body>
    <h2>Table of Factorials</h2>
    <script>
      var fact = 1;

      for (i = 1; i < 10; i++) {
        fact *= i;
        document.write(i + "!=" + fact + "<br>");
      }
    </script>
  </body>
</html>
